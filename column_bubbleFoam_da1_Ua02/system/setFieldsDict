FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha 1
);

regions
(
    boxToCell
    {
        box (-0.05 -0.05 0) (0.05 0.05 0.45);
        fieldValues
        (
            volScalarFieldValue alpha 0
        );
    }
);


