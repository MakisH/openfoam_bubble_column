FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e-5;

boundaryField
{
    atmosphere
    {
        type            fixedValue;
        value           uniform 1e-5;
    }

    bubble
    {
        type zeroGradient;
    }

    wall
    {
        type            buoyantPressure;
        value           uniform 0;
    }

    bottom
    {
      type            buoyantPressure;
      value           uniform 0;
    }

    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //
